<document
  xmlns="http://maven.apache.org/changes/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/changes/1.0.0 http://maven.apache.org/xsd/changes-1.0.0.xsd">
  <body>
    <release
      version="0.1.0"
      description="Entry"
      date="2019-09-13">
      <action type="remove">
        Remove Java Modules.
      </action>
      <action type="update">
        Use semantic versioning.
      </action>
      <action type="update">
        Spring boot 2.1.2.
      </action>
    </release>
    <release
      version="0.0.3"
      description="Vaadin 10"
      date="2019-01-03">
      <action type="remove">
        No JPMS for starter.
      </action>
      <action type="update">
        Use Vaadin 10.
      </action>
    </release>
    <release
      version="0.0.2"
      description="Java 11"
      date="2018-12-29">
      <action type="add">
        Create Java Modules.
      </action>
      <action type="update">
        Update dependencies, parent and plugins to be Java 11 compatible.
      </action>
    </release>
    <release
      version="0.0.1"
      description="bootstrap"
      date="2018-09-10">
      <action type="add">Initial Version.</action>
    </release>
  </body>
</document>